# VideoBGRemover SDK Test Configuration
# Copy this file to .env and fill in your actual values

# Environment: "local" or "prod"
VIDEOBGREMOVER_ENV=local

# Local API (for development)
VIDEOBGREMOVER_LOCAL_API_KEY=vbr_your_local_api_key_here
VIDEOBGREMOVER_LOCAL_BASE_URL=http://localhost:3000

# Production API (use with caution - consumes credits!)
VIDEOBGREMOVER_PROD_API_KEY=vbr_your_production_api_key_here
VIDEOBGREMOVER_PROD_BASE_URL=https://api.videobgremover.com

# Test Videos
TEST_VIDEO_LOCAL_PATH=/path/to/your/test/video.mp4
TEST_VIDEO_URL=https://storage.googleapis.com/example.mp4

# Test Backgrounds (for composition testing)
TEST_BACKGROUND_VIDEO=test_assets/background_video.mp4
TEST_BACKGROUND_IMAGE=test_assets/background_image.png
